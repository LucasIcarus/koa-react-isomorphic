  "scripts": {
    "start": "sh scripts/start",
    "dev": "nodemon --harmony build/server.js",
    "test": "NODE_ENV=test PORT=3001 mocha --compilers js:compiler.js --require co-mocha 'app/**/*-test.js' 'tests/**/*.js'",
    "test:watch": "NODE_ENV=test PORT=3001 mocha --watch --reporter nyan --compilers js:compiler.js --require co-mocha 'app/**/*-test.js' 'tests/**/*.js'",
    "postinstall": "sh scripts/build",
    "test-coverage": "istanbul cover node_modules/.bin/_mocha -- --compilers js:compiler.js --require co-mocha 'app/**/*-test.js' 'tests/**/*.js'",
    "frontend:watch": "webpack-dev-server --config config/webpack/client/development.js",
    "watch": "gulp clean && npm run frontend:watch & gulp backend:watch"
  },
  "keywords": [
    "koa",
    "rxjs",
    "react",
    "redux",
    "ramda",
    "es6",
    "mocha",
    "isomorphic",
    "universal"
