  "scripts": {
    "backend:build": "./scripts/backend-build",
    "backend:watch": "./scripts/backend-watch",
    "build": "yarn run clean && yarn run update-schema && yarn run compile-templates && yarn run frontend:build && yarn run backend:build",
    "clean": "node ./scripts/clean.js",
    "compile-templates": "markoc ./app/server/application/templates",
    "debug": "./scripts/debug",
    "dev": "./scripts/dev",
    "flow:status": "flow status; test $? -eq 0 -o $? -eq 2",
    "flow:stop": "flow stop",
    "flow:watch": "yarn run flow:status && chokidar './app/**/*.js' './app/**/*.jsx' './tests/**/*.js' -c 'yarn run flow:status'",
    "frontend:build": "./scripts/frontend-build",
    "frontend:watch": "./scripts/frontend-watch",
    "pm2:delete": "pm2 delete config/pm2/production.json",
    "pm2:start": "pm2 start config/pm2/production.json",
    "postinstall": "./scripts/postinstall",
    "start": "./scripts/start",
    "test": "./scripts/test",
    "test:coverage": "./scripts/test-coverage",
    "test:lint": "./scripts/test-lint",
    "test:watch": "./scripts/test-watch",
    "update-schema": "mkdir -p build && touch build/schema.json && babel-node ./scripts/update-schema.js",
    "watch": "yarn run clean && yarn run update-schema && (yarn run frontend:watch & yarn run backend:watch)"
  }
