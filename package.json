    "start": "sh scripts/start",
    "debug": "sh scripts/debug",
    "dev": "NODE_ENV=development nodemon prod-server.js",
    "test": "NODE_ENV=test PORT=3001 mocha --compilers js:./compiler './app/**/*-test.js' './tests/**/*.js'",
    "test:watch": "NODE_ENV=test PORT=3001 mocha --watch --reporter nyan --compilers js:./compiler './app/**/*-test.js' './tests/**/*.js'",
    "test:lint": "esw -w 'app/**/*-test.js' 'tests/**/*.js'",
    "coverage": "NODE_ENV=test PORT=3001 istanbul cover ./node_modules/.bin/_mocha -- --compilers js:./compiler './app/**/*-test.js' './tests/**/*.js'",
    "postinstall": "sh scripts/build",
    "frontend:watch": "webpack-dev-server --config config/webpack/client/development.js",
    "update-schema": "babel-node ./scripts/update-schema.js",
    "watch": "gulp clean && npm run frontend:watch & gulp backend:watch"
