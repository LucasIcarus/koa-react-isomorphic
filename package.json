    "test:watch": "NODE_ENV=test PORT=3001 mocha --watch --reporter nyan --compilers js:./compiler './app/**/*-test.js' './tests/**/*.js'",
    "test:lint": "esw -w 'app/**/*-test.js' 'tests/**/*.js'",
